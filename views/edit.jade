html(lang='en')
  // <![endif]
  // BEGIN HEAD
  head
    meta(charset='utf-8')
    title Form Component
    meta(content='width=device-width, initial-scale=1.0', name='viewport')
    meta(content='', name='description')
    meta(content='', name='author')
    link(href='../../assets/bootstrap/css/bootstrap.min.css', rel='stylesheet')
    link(href='../../assets/bootstrap/css/bootstrap-responsive.min.css', rel='stylesheet')
    link(href='../../assets/bootstrap/css/bootstrap-fileupload.css', rel='stylesheet')
    link(href='../../assets/font-awesome/css/font-awesome.css', rel='stylesheet')
    link(href='../../css/style.css', rel='stylesheet')
    link(href='../../css/style-responsive.css', rel='stylesheet')
    link#style_color(href='../../css/style-default.css', rel='stylesheet')
    link(href='../../assets/fancybox/source/jquery.fancybox.css', rel='stylesheet')
    link(rel='stylesheet', type='text/css', href='../../assets/uniform/css/uniform.default.css')
    link(rel='stylesheet', type='text/css', href='../../assets/chosen-bootstrap/chosen/chosen.css')
    link(rel='stylesheet', type='text/css', href='../../assets/jquery-tags-input/jquery.tagsinput.css')
    link(rel='stylesheet', type='text/css', href='../../assets/clockface/css/clockface.css')
    link(rel='stylesheet', type='text/css', href='../../assets/bootstrap-wysihtml5/bootstrap-wysihtml5.css')
    link(rel='stylesheet', type='text/css', href='../../assets/bootstrap-datepicker/css/datepicker.css')
    link(rel='stylesheet', type='text/css', href='../../assets/bootstrap-timepicker/compiled/timepicker.css')
    link(rel='stylesheet', type='text/css', href='../../assets/bootstrap-colorpicker/css/colorpicker.css')
    link(rel='stylesheet', href='../../assets/bootstrap-toggle-buttons/static/stylesheets/bootstrap-toggle-buttons.css')
    link(rel='stylesheet', type='text/css', href='../../assets/bootstrap-daterangepicker/daterangepicker.css')
    link(rel='stylesheet', href='http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css')
  // END HEAD
  // BEGIN BODY
  body.fixed-top
    // BEGIN HEADER
    #header.navbar.navbar-inverse.navbar-fixed-top
      // BEGIN TOP NAVIGATION BAR
      .navbar-inner
        .container-fluid
          // BEGIN LOGO
          a.brand(href='')
            img(src='../../img/logo.png', alt='Metro Lab')
          // END LOGO
          // BEGIN RESPONSIVE MENU TOGGLER
          a#main_menu_trigger.btn.btn-navbar.collapsed(data-toggle='collapse', data-target='.nav-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
            span.arrow
          // END RESPONSIVE MENU TOGGLER
          // END  NOTIFICATION
          .top-nav
            ul.nav.pull-right.top-menu
              li.dropdown
                a.dropdown-toggle(href='/users/logout')
                  span.username LOGOUT
                  i.icon-off
              // END USER LOGIN DROPDOWN
            // END TOP NAVIGATION MENU
      // END TOP NAVIGATION BAR
    // END HEADER
    // BEGIN CONTAINER
    #container.row-fluid
      // BEGIN SIDEBAR
      .sidebar-scroll
        #sidebar.nav-collapse.collapse
          // BEGIN SIDEBAR MENU
          ul.sidebar-menu
            li.sub-menu
              a(href='/users')
                i.icon-dashboard
                span Home
            li.sub-menu.active
              a(href='')
                i.icon-book
                span Upload
            li.sub-menu
              a(href='/users/manage')
                i.icon-cogs
                span Manage File
            li.sub-menu
              a(href='/users/search')
                i.icon-th
                span Search File
          // END SIDEBAR MENU
      // END SIDEBAR
      // BEGIN PAGE
      #main-content
        // BEGIN PAGE CONTAINER
        .container-fluid
          // BEGIN PAGE HEADER
          .row-fluid
            .span12
              // BEGIN THEME CUSTOMIZER
              #theme-change.hidden-phone
                i.icon-cogs
                span.settings
                  span.text Theme Color:
                  span.colors
                    span.color-default(data-style='default')
                    span.color-green(data-style='green')
                    span.color-gray(data-style='gray')
                    span.color-purple(data-style='purple')
                    span.color-red(data-style='red')
              // END THEME CUSTOMIZER
              // BEGIN PAGE TITLE & BREADCRUMB
              h3.page-title
                | FILE UPLOAD
              // END PAGE TITLE & BREADCRUMB
          // END PAGE HEADER
          .row-fluid
            // BEGIN widget
            .widget.green
              .widget-title
                h4
                  i.icon-reorder
                  |  File Upload
              .widget-body.form
                // BEGIN FORM
                form.form-horizontal(name="fileUpload", action="", method="post")
                  .control-group
                    label.control-label File
                    .controls
                      input(name="nama_file", type="text", id="myFile", value="#{data[0].nama_file}", readonly)
                  .control-group
                    label.control-label Status
                    .controls
                      label.radio
                        input.default(type='hidden', name='optionsRadios2', value='option1')
                      label.radio
                        input.default(type='radio', name='status', value='Public', checked)
                        |                                             Public
                      label.radio
                        input.default(type='radio', name='status', value='Private')
                        |                                             Private
                  .control-group
                    label.control-label Deskripsi
                    .controls
                      textarea.span6(rows='3' name="desk") #{data[0].deskripsi}
                  .form-actions
                    button.btn.btn-success(type='submit') Edit
                    button.btn(type='reset') Cancel
                // END FORM
            // END widget
      // END PAGE CONTAINER
    // END PAGE
    // END CONTAINER
    // BEGIN FOOTER
    #footer
      | 2013 &copy; Metro Lab Dashboard.
    // END FOOTER
    // BEGIN JAVASCRIPTS
    // Load javascripts at bottom, this will reduce page load time
    script(src='../../js/jquery-1.8.3.min.js')
    script(src='../../js/jquery.nicescroll.js', type='text/javascript')
    script(type='text/javascript', src='../../assets/jquery-slimscroll/jquery-ui-1.9.2.custom.min.js')
    script(type='text/javascript', src='../../assets/jquery-slimscroll/jquery.slimscroll.min.js')
    script(src='../../assets/fullcalendar/fullcalendar/fullcalendar.min.js')
    script(src='../../assets/bootstrap/js/bootstrap.min.js')
    // ie8 fixes
    //if lt IE 9
      script(src='../../js/excanvas.js')
      script(src='../../js/respond.js')
    script(src='../../assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js', type='text/javascript')
    script(src='../../js/jquery.sparkline.js', type='text/javascript')
    script(src='../../assets/chart-master/Chart.js')
    script(src='../../js/jquery.scrollTo.min.js')
    // common script for all pages
    script(src='../../js/common-scripts.js')
    // script for this page only
    script(src='../../js/easy-pie-chart.js')
    script(src='../../js/sparkline-chart.js')
    script(src='../../js/home-page-calender.js')
    script(src='../../js/home-chartjs.js')
    // END JAVASCRIPTS
  // END BODY
